{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.pdfhubs.site"
        }
      ],
      "destination": "https://pdfhubs.site/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff|woff2|ttf|otf|png|jpg|jpeg|gif|svg|ico|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}